{
    "AWSEBDockerrunVersion": 2,
    "volumes": [
      {
        "name": "api",
        "host": {
          "sourcePath": "./api"
        }
      },
      {
        "name": "web",
        "host": {
          "sourcePath": "./web"
        }
      }
    ],
    "containerDefinitions": [
      {
        "name": "api",
        "image": "bryi/api:latest",
        "environment": [
          {
            "name": "PATH",
            "value": "/home/worker/.local/bin:$PATH"
          },
          {
            "name": "POSTGRES_URL",
            "value": "postgres:5432"
          },
          {
            "name": "POSTGRES_USER",
            "value": "ctm"
          },
          {
            "name": "POSTGRES_PW",
            "value": "ctm"
          },
          {
            "name": "POSTGRES_DB",
            "value": "app_redis"
          },
          {
            "name": "REDIS_SERVER",
            "value": "redis"
          },
          {
            "name": "FLASK_APP",
            "value": "api.py"
          }
        ],
        "essential": true,
        "memory": 128,
        "mountPoints": [
          {
            "sourceVolume": "api",
            "containerPath": "/home/worker/app"
          }
        ],
        "portMappings": [
            {
              "containerPort": 5000
            }
          ]
      },
      {
        "name": "web",
        "image": "bryi/frontend:latest",
        "environment": [
          {
            "name": "PATH",
            "value": "/home/worker/.local/bin:$PATH"
          },
          {
            "name": "FLASK_APP",
            "value": "flask_frontend.py"
          }
        ],
        "essential": true,
        "memory": 128,
        "mountPoints": [
          {
            "sourceVolume": "web",
            "containerPath": "/home/worker/app"
          }
        ],
        "portMappings": [
            {
              "hostPort": 80,
              "containerPort": 5001
            }
          ]
      },
      {
        "name": "nginx-proxy",
        "image": "nginx",
        "essential": true,
        "memory": 128,
        "portMappings": [
          {
            "hostPort": 80,
            "containerPort": 80
          }
        ],
        "links": [
          "php-app"
        ],
        "mountPoints": [
          {
            "sourceVolume": "php-app",
            "containerPath": "/var/www/html",
            "readOnly": true
          },
          {
            "sourceVolume": "nginx-proxy-conf",
            "containerPath": "/etc/nginx/conf.d",
            "readOnly": true
          },
          {
            "sourceVolume": "awseb-logs-nginx-proxy",
            "containerPath": "/var/log/nginx"
          }
        ]
      }
    ]
  }